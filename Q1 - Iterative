void setup() {
  Serial.begin(9600);
  int n = 10;  // Number of terms to display

  int i = 0, j = 1, k = 0, fib;

  Serial.println("Fibonacci Series (Iterative):");
  for (int count = 0; count < n; count++) {
    Serial.print(k);
    Serial.print(" ");
    fib = i + j;
    i = j;
    j = fib;
    k = i;
  }
  Serial.println();
}

void loop() {
  // Nothing here
}
