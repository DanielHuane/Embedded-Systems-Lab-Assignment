int fibonacci_recursive(int n) {
  if (n == 0)
    return 0;
  else if (n == 1)
    return 1;
  else
    return fibonacci_recursive(n - 1) + fibonacci_recursive(n - 2);
}

void setup() {
  Serial.begin(9600);
  int n = 10;  // Number of terms to display

  Serial.println("Fibonacci Series (Recursive):");
  for (int i = 0; i < n; i++) {
    Serial.print(fibonacci_recursive(i));
    Serial.print(" ");
  }
  Serial.println();
}

void loop() {
  // Nothing here
}
